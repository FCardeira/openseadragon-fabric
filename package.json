{
  "name": "openseadragon-fabric",
  "version": "1.0.0",
  "description": "A open sea dragon plugin to use FabricJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clear": "rm -rf ./dist",
    "build": "npm run clear && tsc --declaration",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunoocastro/openseadragon-fabric.git"
  },
  "keywords": [
    "openseadragon",
    "osd",
    "plugin",
    "fabric",
    "fabricjs"
  ],
  "author": "Bruno Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunoocastro/openseadragon-fabric/issues"
  },
  "homepage": "https://github.com/brunoocastro/openseadragon-fabric#readme",
  "devDependencies": {
    "@types/fabric": "^5.3.6",
    "@types/node": "^20.9.2",
    "@types/openseadragon": "^3.0.10",
    "fabric": "^5.3.0",
    "openseadragon": "^4.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
